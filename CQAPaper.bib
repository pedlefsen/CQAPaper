	Pages = {415-418},
	Title = {Retrieval and on-the-fly alignment of sequence fragments from the HIV database},
	Url = {http://bioinformatics.oxfordjournals.org/content/17/5/415.abstract},
	Volume = {17},
	Year = {2001},
	Bdsk-Url-1 = {http://bioinformatics.oxfordjournals.org/content/17/5/415.abstract},
