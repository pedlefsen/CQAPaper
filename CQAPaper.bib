@article{Jordan:2003,
	Abstract = {
Transposable elements (TEs) are abundant in mammalian genomes and have potentially contributed to their hosts' evolution by providing novel regulatory or coding sequences. We surveyed different classes of regulatory region in the human genome to assess systematically the potential contribution of TEs to gene regulation. Almost 25\% of the analyzed promoter regions contain TE-derived sequences, including many experimentally characterized cis-regulatory elements. Scaffold/matrix attachment regions (S/MARs) and locus control regions (LCRs) that are involved in the simultaneous regulation of multiple genes also contain numerous TE-derived sequences. Thus, TEs have probably contributed substantially to the evolution of both gene-specific and global patterns of human gene regulation.},
	Author = {I. King Jordan and Igor B. Rogozin and Galina V. Glazko and Eugene V. Koonin},
	Date-Added = {2009-04-27 16:38:26 -0400},
	Date-Modified = {2009-04-27 16:38:26 -0400},
	Doi = {DOI: 10.1016/S0168-9525(02)00006-9},
	Issn = {0168-9525},
	Journal = {Trends in Genetics},
	Number = {2},
	Pages = {68 - 72},
	Title = {Origin of a substantial fraction of human regulatory sequences from transposable elements},
	Volume = {19},
	Year = {2003},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/B6TCY-47DT7P2-1/2/b66308c198ebb74738ce4a981fba3be7},
	Bdsk-Url-2 = {http://dx.doi.org/10.1016/S0168-9525(02)00006-9}}

@article{Piriyapongsa:2007,
	Abstract = {We sought to evaluate the extent of the contribution of transposable elements (TEs) to human microRNA (miRNA) genes along with the evolutionary dynamics of TE-derived human miRNAs. We found 55 experimentally characterized human miRNA genes that are derived from TEs, and these TE-derived miRNAs have the potential to regulate thousands of human genes. Sequence comparisons revealed that TE-derived human miRNAs are less conserved, on average, than non-TE-derived miRNAs. However, there are 18 TE-derived miRNAs that are relatively conserved, and 14 of these are related to the ancient L2 and MIR families. Comparison of miRNA vs. mRNA expression patterns for TE-derived miRNAs and their putative target genes showed numerous cases of anti-correlated expression that are consistent with regulation via mRNA degradation. In addition to the known human miRNAs that we show to be derived from TE sequences, we predict an additional 85 novel TE-derived miRNA genes. TE sequences are typically disregarded in genomic surveys for miRNA genes and target sites; this is a mistake. Our results indicate that TEs provide a natural mechanism for the origination miRNAs that can contribute to regulatory divergence between species as well as a rich source for the discovery of as yet unknown miRNA genes.
},
	Author = {Piriyapongsa, Jittima and Marino-Ramirez, Leonardo and Jordan, I. King},
	Date-Added = {2009-04-27 16:27:03 -0400},
	Date-Modified = {2009-04-27 16:27:03 -0400},
	Doi = {10.1534/genetics.107.072553},
	Eprint = {http://www.genetics.org/cgi/reprint/176/2/1323.pdf},
	Journal = {Genetics},
	Number = {2},
	Pages = {1323-1337},
	Title = {{Origin and Evolution of Human microRNAs From Transposable Elements}},
	Volume = {176},
	Year = {2007},
	Bdsk-Url-1 = {http://www.genetics.org/cgi/content/abstract/176/2/1323},
